{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "paperTrail": {
      "enabled": true
    }
  },
  "attributes": {
    "title": {
      "pluginOptions": {},
      "type": "string",
      "required": true
    },
    "slug": {
      "pluginOptions": {},
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "url": {
      "pluginOptions": {},
      "type": "string",
      "unique": true
    },
    "excerpt": {
      "pluginOptions": {},
      "type": "text"
    },
    "admin_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "admin::user"
    },
    "urls": {
      "displayName": "originalUrls",
      "type": "component",
      "repeatable": true,
      "pluginOptions": {},
      "component": "url.original-urls"
    },
    "authors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::author.author",
      "inversedBy": "articles"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {}
    },
    "subscribers": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "admin::user"
    },
    "origin": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::magazine.magazine",
      "inversedBy": "articles"
    },
    "articleData": {
      "type": "string"
    },
    "language": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::language.language",
      "inversedBy": "articles"
    },
    "translation_requests": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::translation-request.translation-request",
      "mappedBy": "article"
    }
  }
}
