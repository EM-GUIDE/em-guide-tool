{
    "kind": "collectionType",
    "collectionName": "articles",
    "info": {
        "singularName": "article",
        "pluralName": "articles",
        "displayName": "Article",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {
        "i18n": {
            "localized": true
        },
        "paperTrail": {
            "enabled": true
        }
    },
    "attributes": {
        "title": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "string",
            "required": true
        },
        "slug": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "uid",
            "targetField": "title",
            "required": true
        },
        "url": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "string",
            "unique": true
        },
        "excerpt": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "text"
        },
        "admin_user": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "admin::user"
        },
        "urls": {
            "displayName": "originalUrls",
            "type": "component",
            "repeatable": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "component": "url.original-urls"
        },
        "translation_requests": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::translation-request.translation-request",
            "mappedBy": "article"
        },
        "authors": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::author.author",
            "inversedBy": "articles"
        },
        "images": {
            "type": "media",
            "multiple": true,
            "required": false,
            "allowedTypes": [
                "images"
            ],
            "pluginOptions": {
                "i18n": {
                    "localized": false
                }
            }
        },
        "subscribers": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "admin::user"
        },
        "origin": {
            "type": "relation",
            "relation": "manyToOne",
            "target": "api::magazine.magazine",
            "inversedBy": "articles"
        }
    }
}
