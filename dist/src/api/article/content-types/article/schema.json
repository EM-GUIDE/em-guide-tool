{
    "kind": "collectionType",
    "collectionName": "articles",
    "info": {
        "singularName": "article",
        "pluralName": "articles",
        "displayName": "Article",
        "description": ""
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {
        "i18n": {
            "localized": true
        },
        "paperTrail": {
            "enabled": true
        }
    },
    "attributes": {
        "title": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "string",
            "required": true
        },
        "slug": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "uid",
            "targetField": "title",
            "required": true
        },
        "url": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "string",
            "unique": true
        },
        "excerpt": {
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "type": "text"
        },
        "status": {
            "pluginOptions": {
                "i18n": {
                    "localized": false
                }
            },
            "type": "enumeration",
            "enum": [
                "open",
                "claimed",
                "in_progress",
                "in_review",
                "flagged",
                "accepted"
            ],
            "default": "open",
            "required": true
        },
        "admin_user": {
            "type": "relation",
            "relation": "oneToOne",
            "target": "admin::user"
        },
        "urls": {
            "displayName": "originalUrls",
            "type": "component",
            "repeatable": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "component": "url.original-urls"
        },
        "translation_requests": {
            "type": "relation",
            "relation": "oneToMany",
            "target": "api::translation-request.translation-request",
            "mappedBy": "article"
        },
        "comments": {
            "displayName": "comments",
            "type": "component",
            "repeatable": true,
            "pluginOptions": {
                "i18n": {
                    "localized": true
                }
            },
            "component": "comments.comments"
        },
        "authors": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::author.author",
            "inversedBy": "articles"
        }
    }
}
